{
  "name": "vega-editor",
  "version": "0.31.0",
  "homepage": "http://vega.github.io/editor",
  "private": true,
  "devDependencies": {
    "@types/node": "^11.13.6",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.7.0",
    "autoprefixer": "^9.5.1",
    "awesome-typescript-loader": "^5.2.1",
    "css-loader": "^2.1.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint": "^5.16.0",
    "gh-pages": "^2.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "monaco-editor-webpack-plugin": "^1.7.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.17.0",
    "style-loader": "^0.23.1",
    "typescript": "^3.4.4",
    "webpack": "4.30.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1"
  },
  "dependencies": {
    "@types/react": "^16.8.14",
    "@types/react-redux": "^7.0.8",
    "ajv": "^6.10.0",
    "d3-geo-projection": "^2.6.0",
    "d3-request": "1.0.6",
    "history": "^4.9.0",
    "json-stringify-pretty-compact": "^2.0.0",
    "lz-string": "^1.4.4",
    "monaco-editor": "^0.16.2",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-clipboard.js": "^2.0.7",
    "react-dom": "^16.8.6",
    "react-feather": "^1.1.6",
    "react-ga": "2.5.7",
    "react-monaco-editor": "0.25.1",
    "react-paginate": "^6.3.0",
    "react-portal": "^4.2.0",
    "react-redux": "^7.0.2",
    "react-router-dom": "^5.0.0",
    "react-select": "^2.4.3",
    "react-split-pane": "^0.1.87",
    "react-tooltip": "^3.10.0",
    "redux": "^4.0.1",
    "redux-localstorage": "^0.4.1",
    "redux-thunk": "^2.3.0",
    "tslib": "^1.9.3",
    "vega-cli": "5.3.5",
    "vega-datasets": "^1.25.0",
    "vega-lite": "3.2.1",
    "vega-schema-url-parser": "^1.1.0",
    "vega-tooltip": "^0.17.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{js,jsx,ts,tsx,css,scss}": [
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "start": "webpack-dev-server --mode development --hot",
    "start:production": "webpack-dev-server --mode production",
    "build": "npm run clean && npm run vendor && webpack --mode production --progress",
    "clean": "rm -rf dist public/spec public/data",
    "postbuild": "rsync -r --ignore-existing public/* dist",
    "generate-example-images": "./scripts/generate-example-images.sh",
    "postinstall": "npm run vendor",
    "vendor": "./scripts/vendor.sh",
    "eslintbase": "eslint '{src,test}/**/*.ts' '*.js' '*.tsx'",
    "prettierbase": "prettier '*.scss' '*.html'",
    "format": "npm run eslintbase -- --fix && npm run prettierbase -- --write",
    "lint": "npm run eslintbase && npm run prettierbase -- --check",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "analyze": "webpack --mode production --profile --json > stats.json && webpack-bundle-analyzer stats.json"
  }
}
